<h2>Section List</h2>

<table>
  <thead>
    <tr>
      <th>Section</th>
      <th>Count</th>
    </tr>
  </thead>

  <tbody>
    <% @section_count.each do |k,v| %>
      <tr>
        <td><%= link_to k, admin_user_list_path(:section => k) %></td>
        <td><%= v %></td>
      </tr>
    <% end %>
  </tbody>
</table>